                                <td data-bind="text: DocDate"></td>
                                <td data-bind="text: MahramanehName"></td>
                                <td data-bind="text: Eghdam"></td>
                                <td data-bind="text: Tanzim"></td>
                                <td data-bind="text: AmalDate"></td>
                                <td data-bind="text: MhltDate"></td>
                                <td data-bind="text: EndDate"></td>
                                <td data-bind="text: CustCode"></td>
                                <td data-bind="text: CustName"></td>
                                <td data-bind="text: DocDesc"></td>
                                <td data-bind="text: EghdamComm"></td>
                                <td data-bind="text: FinalComm"></td>
                                <!-- <td data-bind="text: SpecialComm"></td>-->
