@{
    /**/

    ViewBag.Title = "ErjDocK";
    Layout = "~/Views/Shared/_LayoutNew.cshtml";
}

<div class="row clearfix">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="card">
            <!--border: 1px solid #eb8020;-->
            <div class="header">
                <strong id="">فهرست پرونده ها</strong>
            </div>
            <div class="body" style="padding-left: 15px;padding-right: 15px;">
                <form class="clearfix">
                    <div class="row form-inline ">
                        <div class="col-sm-3 form-inline">
                            <div class="col-sm-6">
                                <div class="input-group">
                                    <div class="input-group-addon">
                                        <span class="input-group-btn">
                                            <a id="btnaztarikh">
                                                <img src="~/Content/img/list/calendar.png" class="icon" height="20" width="20" />
                                            </a>
                                        </span>
                                    </div>
                                    <div class="form-group form-float">
                                        <div class="form-line focused fix">
                                            <input id="aztarikh" data-bind="value:AzDate" class="form-control date" type="text" maxlength="10">
                                            <label class="form-label" for="">از تاریخ</label>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-sm-6">
                                <div class="input-group">
                                    <div class="input-group-addon">
                                        <span class="input-group-btn">
                                            <a id="btntatarikh">
                                                <img src="~/Content/img/list/calendar.png" class="icon" height="20" width="20" />
                                            </a>
                                        </span>
                                    </div>
                                    <div class="form-group form-float">
                                        <div class="form-line focused fix">
                                            <input id="tatarikh" data-bind="value:TaDate" class="form-control date" type="text" maxlength="10">
                                            <label class="form-label" for="">تا تاریخ</label>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>


                        <div class="col-sm-3"> 
                            <div class="input-group">
                                <div class="input-group-addon">
                                    <a id="btnErjCust" class="dropdown-toggle" data-toggle="modal" data-target="#modal-ErjCust">
                                        <img src="~/Content/img/list/man.png" class="icon" height="20" width="20" />
                                    </a>
                                </div>
                                <div class="form-group form-float" style="margin-left: 250px;">
                                    <div class="form-line focused fix">
                                        <input id="nameErjCust" class="form-control" readonly />
                                        <label class="form-label"> مشتری</label>
                                    </div>
                                </div>

                            </div>
                        </div>
                        <div class="col-sm-3">
                            <div class="input-group">
                                <div class="input-group-addon">
                                    <a id="btnKhdt" class="dropdown-toggle" data-toggle="modal" data-target="#modal-Khdt">
                                        <img src="~/Content/img/list/SearchKala.png" class="icon" height="20" width="20" />
                                    </a>
                                </div>
                                <div class="form-group form-float" style="margin-left: 250px;">
                                    <div class="form-line focused fix">
                                        <input id="nameKhdt" class="form-control" readonly />
                                        <label class="form-label"> نوع کار</label>
                                    </div>
                                </div>

                            </div>
                        </div>

                        <div class="col-sm-3">
                            <div class="form-inline" style="margin-left: 5px; margin-right: 5px;">
                                <div class="form-group">
                                    <select id="status" data-bind="
                                   options: ErjStatusList,
                                   optionsText: function(item) {return item.Status},
                                   optionsValue: 'Status',
                                   optionsCaption: 'همه موارد'"></select>
                                </div>
                            </div>
                        </div>

                        <br />
                        <br />
                        <br />
                        <br />


                        <div class="col-sm-10" id="ViewSrchSt">
                            <div class="form-group form-float">
                                <div class="form-line">
                                    <input id="SrchSt" class="form-control string" />
                                    <label class="form-label active">جستجو برای</label>
                                </div>
                            </div>
                        </div>

                        <div class="pull-left">
                            <button type="button" id="CreateReport" class="btn btn-primary"><i class="fa fa-sign-in fa-lg fa-fw"></i>گزارش گیری</button>
                        </div>

                    </div>
                </form>
                <hr />
                <div style="height:425px;overflow:auto;border: 1px #ddd solid;" class="table-responsive-md">
                    <table class="table table-hover table-striped">
                        <thead style="cursor: pointer;">
                            <tr data-bind="click: sortTableDocK">
                                <th data-column="DocNo">
                                    شماره پرونده <span data-bind="attr: { class: currentColumn() == 'DocNo' ? 'isVisible' : 'isHidden' }"> <i data-bind="attr: { class: iconTypeDocNo }"></i> </span>
                                </th>
                                <th data-column="DocDate" style="width:500px">
                                    تاريخ پرونده <span data-bind="attr: { class: currentColumn() == 'DocDate' ? 'isVisible' : 'isHidden' }"> <i data-bind="attr: { class: iconTypeDocDate }"></i> </span>
                                </th>
                                <th data-column="MahramanehName">
                                    نوع <span data-bind="attr: { class: currentColumn() == 'MahramanehName' ? 'isVisible' : 'isHidden' }"> <i data-bind="attr: { class: iconTypeMahramanehName }"></i> </span>
                                </th>
                                <th data-column="Eghdam">
                                    اقدام  <span data-bind="attr: { class: currentColumn() == 'Eghdam' ? 'isVisible' : 'isHidden' }"> <i data-bind="attr: { class: iconTypeEghdam }"></i> </span>
                                </th>
                                <th data-column="Tanzim">
                                    تنظيم <span data-bind="attr: { class: currentColumn() == 'Tanzim' ? 'isVisible' : 'isHidden' }"> <i data-bind="attr: { class: iconTypeTanzim }"></i> </span>
                                </th>
                                <th data-column="AmalDate">
                                    تاريخ عمل <span data-bind="attr: { class: currentColumn() == 'AmalDate' ? 'isVisible' : 'isHidden' }"> <i data-bind="attr: { class: iconTypeAmalDate }"></i> </span>
                                </th>
                                <th data-column="MhltDate">
                                    مهلت پرونده <span data-bind="attr: { class: currentColumn() == 'MhltDate' ? 'isVisible' : 'isHidden' }"> <i data-bind="attr: { class: iconTypeMhltDate }"></i> </span>
                                </th>
                                <th data-column="EndDate">
                                    تاريخ اتمام پرونده<span data-bind="attr: { class: currentColumn() == 'EndDate' ? 'isVisible' : 'isHidden' }"> <i data-bind="attr: { class: iconTypeEndDate }"></i> </span>
                                </th>
                                <th data-column="CustCode">
                                    کد مشتري  <span data-bind="attr: { class: currentColumn() == 'CustCode' ? 'isVisible' : 'isHidden' }"> <i data-bind="attr: { class: iconTypeCustCode }"></i> </span>
                                </th>
                                <th data-column="CustName">
                                    نام مشتري <span data-bind="attr: { class: currentColumn() == 'CustName' ? 'isVisible' : 'isHidden' }"> <i data-bind="attr: { class: iconTypeCustName }"></i> </span>
                                </th>
                                <th data-column="DocDesc">
                                    توضيحات عمومي<span data-bind="attr: { class: currentColumn() == 'DocDesc' ? 'isVisible' : 'isHidden' }"> <i data-bind="attr: { class: iconTypeDocDesc }"></i> </span>
                                </th>
                                <th data-column="EghdamComm">
                                    توضيحات اقدام<span data-bind="attr: { class: currentColumn() == 'EghdamComm' ? 'isVisible' : 'isHidden' }"> <i data-bind="attr: { class: iconTypeEghdamComm }"></i> </span>
                                </th>
                                <th data-column="FinalComm">
                                    توضيحات نهايي<span data-bind="attr: { class: currentColumn() == 'FinalComm' ? 'isVisible' : 'isHidden' }"> <i data-bind="attr: { class: iconTypeFinalComm }"></i> </span>
                                </th>
                                <!-- <th data-column="SpecialComm">
        توضيحات مديران <span data-bind="attr: { class: currentColumn() == 'SpecialComm' ? 'isVisible' : 'isHidden' }"> <i data-bind="attr: { class: iconTypeSpecialComm }"></i> </span>
    </th>-->
                                <th data-column="Status">
                                    وضعيت <span data-bind="attr: { class: currentColumn() == 'Status' ? 'isVisible' : 'isHidden' }"> <i data-bind="attr: { class: iconTypeStatus }"></i> </span>
                                </th>
                                <th data-column="Spec">
                                    ملاحظات<span data-bind="attr: { class: currentColumn() == 'Spec' ? 'isVisible' : 'isHidden' }"> <i data-bind="attr: { class: iconTypeSpec }"></i> </span>
                                </th>
